                                        <label class="col-sm-1 control-label" for="WWNewAccountAddress">地址*</label>
                                        <div class="col-sm-8">
                                            <input class="form-control" type="text" id="WWNewAccountAddress" placeholder="地址">
                                        </div>
                                        @*<label class="col-sm-1 control-label" for="WWZipCode">邮政编码</label>
                                        <div class="col-sm-2">
                                            <input class="form-control" type="text" id="WWZipCode" placeholder="邮政编码">
                                        </div>*@
                                    </div>
                                </div>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td style="border-top:0;border-bottom:0;">
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        <label class="col-sm-1 control-label" for="country2">国家</label>
                                        <div class="col-sm-5">
                                            <select class="form-control" id="country2"></select>
                                        </div>
                                        <label class="col-sm-1 control-label" for="WWNewAccountProvince">州/省</label>
                                        <div class="col-sm-5">
                                            <input class="form-control" type="text" id="WWNewAccountProvince" placeholder="州/省">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td style="border-top:0;border-bottom:0;">
                            <div class="row">
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        <label class="col-sm-1 control-label" for="WWNewAccountPhone">手机号码</label>
