                                            <label class="col-sm-2 control-label" for="WWEmail">工作电子邮件地址</label>
                                            <div class="col-sm-5">
                                                <input class="form-control" type="text" id="WWEmail" placeholder="工作电子邮件地址" readonly="readonly">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </td>
                        </tr>*@
                    </table>

                <table class="table table-bordered">
                    <tr>
                        <td style="border-bottom:0;">
                            <div class="row">
                                <div class="col-sm-12">
                                    <span style="border-bottom:solid 1px;">新的客户信息：</span>
                                </div>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td style="border-top:0;border-bottom:0;">
