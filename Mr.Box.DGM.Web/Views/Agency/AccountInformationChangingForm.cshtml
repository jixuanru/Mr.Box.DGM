@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>客户信息变更表</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    @Scripts.Render("~/jquery")
    @Scripts.Render("~/bootstrap")
    <link href="~/Content/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/js/Country.js"></script>
    <script src="~/Scripts/js/AccountInformationChangingForm.js"></script>
    <script type="text/javascript">
        $(function () {
            $($(".topcontainer .nav li")[3]).addClass("active");
            InitTables();

            $(".WebWithdrawalsSubmitbtn").click(function () {
                if (CheckWebApplyData()) {
                    SubmitWebApplyData();
                }
            });
            $("#modalwithdraw")
                .click(function () {
                    $("#modal_withdraw").modal('hide');
                });
        })

    </script>
    <style>
        .modal_withdrawico {
            background: url('../../Content/images/dialog-icons.gif') 0 0 no-repeat;
            width: 48px;
            height: 48px;
            border: 0;
        }

        .modal_withdraw_Con {
            margin-top: 15px;
            display: block;
        }
    </style>
</head>
<body>
    @{Html.RenderPartial("Top");}

    <div class="container mTop">

        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title text-center">客户信息变更表</h3>
            </div>

            <div class="panel-body">
